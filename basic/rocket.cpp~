#include<iostream>
#include<string>

using namespace std;
typedef long long ll;

int main(){
	cout<<"int型のサイズ："<<sizeof(int)<<endl;
	cout<<"long long型のサイズ："<<sizeof(ll)<<endl;
	cout<<"double型のサイズ："<<sizeof(double)<<endl;
	cout<<"char型のサイズ："<<sizeof(char)<<endl;
	cout<<"string型のサイズ："<<sizeof(string)<<endl;

return 0;
}
